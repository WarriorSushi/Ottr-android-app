# Copy this file to signing.properties and fill in your keystore details
# DO NOT commit the actual signing.properties file with real credentials to version control

# Path to your keystore file
storeFile=path/to/keystore.jks

# Keystore password
storePassword=your_keystore_password

# Key alias
keyAlias=your_key_alias

# Key password
keyPassword=your_key_password
